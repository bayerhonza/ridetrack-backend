<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN" monitorInterval="15" name="DefaultLoggingConfig">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <Logger name="com.ensimag.ridetrack" level="TRACE"/>
        <Root level="error">
            <AppenderRef ref="Console"/>
        </Root>
    </Loggers>
</Configuration>